#!/bin/sh
mkdir -p results
if [ -f /usr/ucb/touch ] ; then
	TOUCH=/usr/ucb/touch
else
	if [ -f /usr/bin/touch ] ; then
		TOUCH=/usr/bin/touch
	else
		if [ -f /bin/touch ] ; then
			TOUCH=/bin/touch
		fi
	fi
fi
echo "$1...";
/bin/cp /dev/null results/$1
case $3 in
ipf)
	../ipf -Rnvf regress/$1 2>/dev/null > results/$1
	status=$?
	;;
ipftest)
	../ipftest -D -r regress/$1 -i /dev/null > results/$1
	status=$?
	;;
esac
if [ $status -eq 0 ] ; then
	cmp expected/$1 results/$1
	status=$?
fi
if [ $status = 0 ] ; then
	$TOUCH $1
fi
exit $status
